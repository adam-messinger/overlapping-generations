{
  "name": "Net Zero 2050",
  "description": "IEA Net Zero pathway. High carbon pricing, accelerated renewables deployment, rapid electrification across all sectors.",

  "energy": {
    "carbonPrice": 150,
    "sources": {
      "solar": { "alpha": 0.40, "growthRate": 0.30 },
      "wind": { "alpha": 0.28, "growthRate": 0.22 },
      "battery": { "alpha": 0.30 },
      "nuclear": { "growthRate": 0.04 }
    }
  },

  "demand": {
    "efficiencyMultiplier": 1.3,
    "sectors": {
      "transport": { "costEscalationThreshold": 0.75, "costEscalationRate": 1.5 },
      "buildings": { "costEscalationThreshold": 0.90, "costEscalationRate": 1.0 },
      "industry": { "costEscalationThreshold": 0.65, "costEscalationRate": 2.0 }
    }
  },

  "climate": {
    "sensitivity": 3.0
  }
}
